{
  "name": "@teei/entitlements",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "TEEI Entitlements Policy Engine - Feature access control and ABAC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@teei/shared-schema": "workspace:*",
    "@teei/shared-utils": "workspace:*",
    "drizzle-orm": "^0.29.3",
    "ioredis": "^5.3.2",
    "postgres": "^3.4.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
