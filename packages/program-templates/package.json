{
  "name": "@teei/program-templates",
  "version": "0.1.0",
  "description": "Program template schemas and defaults for TEEI CSR Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schemas/*": {
      "types": "./dist/schemas/*.d.ts",
      "default": "./dist/schemas/*.js"
    }
  }
}
