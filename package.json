{
  "name": "teei-csr-platform",
  "version": "1.0.0",
  "private": true,
  "description": "TEEI CSR Platform - Unified impact measurement ecosystem",
  "scripts": {
    "dev": "concurrently -n gateway,profile,kintell,buddy,buddy-conn,upskilling,q2q,safety,cockpit \"pnpm --filter @teei/api-gateway dev\" \"pnpm --filter @teei/unified-profile dev\" \"pnpm --filter @teei/kintell-connector dev\" \"pnpm --filter @teei/buddy-service dev\" \"pnpm --filter @teei/buddy-connector dev\" \"pnpm --filter @teei/upskilling-connector dev\" \"pnpm --filter @teei/q2q-ai dev\" \"pnpm --filter @teei/safety-moderation dev\" \"pnpm --filter @teei/corp-cockpit-astro dev\"",
    "build": "pnpm -r --filter './packages/**' build && pnpm -r --filter './services/**' build && pnpm -r --filter './apps/**' build",
    "test": "pnpm -r test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:unit": "pnpm -r test:unit",
    "test:integration": "pnpm -r test:integration",
    "test:coverage": "pnpm -r test -- --coverage",
    "test:cov": "vitest --coverage --run",
    "test:seed": "tsx tests/fixtures/seed.ts seed",
    "test:seed:reset": "tsx tests/fixtures/seed.ts reset",
    "e2e:run": "playwright test --project=chromium",
    "e2e:coverage": "node scripts/e2e-coverage.js",
    "vrt:baseline": "cd apps/corp-cockpit-astro && playwright test visual-comprehensive --update-snapshots",
    "vrt:check": "cd apps/corp-cockpit-astro && playwright test visual-comprehensive",
    "a11y:ci": "cd apps/corp-cockpit-astro && playwright test tests/a11y/accessibility.spec.ts",
    "k6:run": "k6 run tests/load/dashboard-load.js && k6 run tests/load/reporting-load.js",
    "quality:report": "tsx scripts/quality-report.ts",
    "quality:report:html": "tsx scripts/quality-report.ts --format=html",
    "quality:report:json": "tsx scripts/quality-report.ts --format=json",
    "quality:report:md": "tsx scripts/quality-report.ts --format=md",
    "coverage:analyze": "tsx scripts/coverage-analysis.ts",
    "coverage:gaps": "tsx scripts/coverage-analysis.ts --output=reports/quality/coverage-gaps.md",
    "flakiness:analyze": "tsx tests/utils/flakiness-tracker.ts analyze",
    "flakiness:report": "tsx tests/utils/flakiness-tracker.ts report",
    "flakiness:quarantine": "tsx tests/utils/flakiness-tracker.ts quarantine",
    "db:migrate": "pnpm --filter @teei/shared-schema db:migrate",
    "db:seed": "pnpm --filter @teei/shared-schema db:seed",
    "db:reset": "pnpm --filter @teei/shared-schema db:reset",
    "db:studio": "pnpm --filter @teei/shared-schema db:studio",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.astro",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,astro,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,astro,json,md}\"",
    "typecheck": "pnpm -r typecheck",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "demo:create": "tsx scripts/demo/create.ts",
    "demo:refresh": "tsx scripts/demo/refresh.ts",
    "demo:teardown": "tsx scripts/demo/teardown.ts",
    "demo:list": "tsx scripts/demo/list.ts",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "keywords": [
    "csr",
    "impact-measurement",
    "social-impact",
    "sroi",
    "volunteer-management"
  ],
  "author": "TEEI Platform (Henrik RÃ¸ine)",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@chromatic-com/storybook": "^1.0.0",
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-a11y": "^7.6.7",
    "@storybook/addon-coverage": "^1.0.0",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.1",
    "chromatic": "^10.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-storybook": "^0.6.15",
    "glob": "^10.3.10",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.12.2",
    "sharp": "^0.34.5",
    "storybook": "^7.6.7",
    "terser": "^5.44.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
