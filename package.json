{
  "name": "teei-csr-platform",
  "version": "0.1.0",
  "private": true,
  "description": "TEEI CSR Platform - Unified impact measurement and AI-powered ecosystem",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.astro",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,astro,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,astro,json,md}\"",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "keywords": [
    "csr",
    "impact-measurement",
    "social-impact",
    "sroi",
    "volunteer-management"
  ],
  "author": "TEEI Platform (Henrik RÃ¸ine)",
  "license": "PROPRIETARY",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.2",
    "typescript": "^5.3.3"
  }
}
